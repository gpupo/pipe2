{
    "name": "gpupo/pipe2",
    "description": "Convert Google Shopping XML format to XMLPipe2 format",
    "keywords": ["sphinx", "xml", "xmlpipe2"],
    "license": "MIT",
    "require": {
        "symfony/console": "dev-master",
        "ext-xml": "*",
        "gpupo/common-schema": "dev-master",
        "cocur/slugify": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": { "Gpupo\\": "src/Gpupo" }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\": "tests/Gpupo/Tests"
        }
    },
    "minimum-stability": "dev"
}
