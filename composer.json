{
    "name": "gpupo/pipe2",
    "description": "Convert Google Shopping XML format to XMLPipe2 format",
    "homepage": "https://opensource.gpupo.com/pipe2/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["sphinx", "xml", "xmlpipe2"],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "ext-xml": "*",
        "symfony/console": "~2.3",
        "gpupo/common": "~1.3",
        "gpupo/common-schema": "~1.0",
        "cocur/slugify": "~1.0",
        "twig/twig" : "~1.0"
    },
    "autoload": {
        "psr-4": { "Gpupo\\Pipe2\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\Pipe2\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability" : "stable"
}
